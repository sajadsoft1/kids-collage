{
    "name": "karnoweb/laravel-notification",
    "description": "Multi-channel notification business layer for Laravel",
    "keywords": ["laravel", "notification", "channels", "sms", "email", "database"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Karnoweb",
            "homepage": "https://github.com/karnoweb"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/notifications": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Karnoweb\\LaravelNotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karnoweb\\LaravelNotification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karnoweb\\LaravelNotification\\LaravelNotificationServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
